{% extends "en_base.html" %} 
 {% block content %}
 <title>Generate QuantiMAt 2.0 Probesets lists</title>
 <script src="../static/js/jquery.base64.js"></script>
 <script src="../static/js/tableExport.js"></script>
<div class="panel panel-info" >
<div class="panel-heading">
        <b>Generate QuantiMAT 2.0 Probesets Lists（Save as a xls file format）</b>
</div>
<div class="container" style="padding:20px;height:auto;"> 
<table id="tb_pl_m" class="table table-bordered">
<thead>
<tr>
<th>Probe Number</th>
<th>Sequence</th>
<th>Function</th>
</tr>
</thead>
<tbody>
{% for cfl in CE_final_list %}
<tr>
<td>{{ cfl.0 }}</td>
<td>{{ cfl.1 }}</td>
<td>CE</td>
</tr>
{%endfor%}
{% for lffl in LE_final_final_list %}
<tr>
<td>{{ lffl.0 }}</td>
<td>{{ lffl.1 }}</td>
<td>LE</td>
</tr>
{%endfor%}
{% for bfl in BL_final_list %}
<tr>
<td>{{ bfl.0 }}</td>
<td>{{ bfl.1 }}</td>
<td>BL</td>
</tr>
{%endfor%}
</tbody>
<tfoot>
<tr>
<td>-None-</td><td>-None-</td><td>-None-</td>
</tr>
</tfoot>
</table>
<div style="text-align:center;">
<button type="submit" onclick="$('#tb_pl_m').tableExport({ type: 'excel', separator: ';', escape: 'false' });"  class="btn btn-primary" style="text-shadow: black 5px 3px 3px;"><span class="glyphicon glyphicon-file"></span>Export Probesets Lists</button>
</div>
</div>
</div>
<!-- /#mainWrap -->
{% endblock %}
}
#dvFormBG
{
	width:100%;
	height:100%;
	z-index:4;
	position:absolute;
	top:0px;
	left:0px;	
	background-color:Black;
	filter:progid:DXImageTransform.Microsoft.Alpha(opacity=30);
	filter:alpha(opacity=30);  /*IE5、IE5.5、IE6、IE7*/
	opacity: .3;/*Opera9.0+、Firefox1.5+、Safari、Chrome*/
	-moz-opacity:0.30;
}
.TogglePara_body{padding-left:20px; padding-top:4px; padding-right:4px; padding-bottom:4px; border-left:0px solid white; vertical-align:top;background-color:#fff6dc;}
